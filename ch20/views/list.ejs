<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Belajar</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
    <h1>WELCOME1</h1>
        <div class="container" class="col-sm-12">
    <form>

        
    <!---------Menampilkan box String------->
    <div class="row">
        <div class="col-sm-2">
            <div class="form-check">
                <input class="form-check-input" type="checkbox" />String
            </div>
        </div>
    </div>
    <div class="col-sm-10">
        <div class="form-group">
            <input type="text" class="form-control" id="String" name="String"
            placeholder="String" />
        </div>
    </div>
    <!---------Menampilkan box Integer------->
    <div class="row">
        <div class="col-sm-2">
            <div class="form-check">
                <input class="form-check-input" type="checkbox" />Integer
            </div>
        </div>
    </div>
    <div class="col-sm-10">
        <div class="form-group">
            <input type="text" class="form-control" id="Integer" name="Integer"
            placeholder="Integer" />
        </div>
    </div>
    <!---------Menampilkan box Float------->
    <div class="row">
        <div class="col-sm-2">
            <div class="form-check">
                <input class="form-check-input" type="checkbox" />Float
            </div>
        </div>
    </div>
    <div class="col-sm-10">
        <div class="form-group">
            <input type="text" class="form-control" id="Float" name="Float"
            placeholder="Float" />
        </div>
    </div>
    <!---------Menampilkan box Date------->
    <div class="row">
        <div class="col-sm-2">
            <div class="form-check">
                <input class="form-check-input" type="checkbox" />Date
            </div>
        </div>
    </div>
    <div class="col-sm-10">
        <div class="form-group">
            <input type="date" class="form-control" id="Date" name="Date"
            placeholder="Date" />
            TO
            <input type="" class="form-control" id="Date" name="Date"
            placeholder="Date" />
        </div>
    </div>

    <!---------Menampilkan box Bolean------->
    <div class="row">
        <div class="col-sm-2">
            <div class="form-check">
                <input class="form-check-input" type="checkbox" />Boolean
            </div>
        </div>
    </div>
    <div class="col-sm-10">
        <div class="form">
            <select id_type="month" name="boolean">
                <option value="">--pilih type--</option>
                <option value="true">true</option>
                <option value="false">false</option>
            </select>
        </div>
    </div>
    
    <a class="btn btn-primary" href="/<%= data.id %>">SEARCH</a>
</div>

</form>
    <table class="table table-striped table-dark" border="1">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Umur</th>
            <th scope="col">Integer</th>
            <th scope="col">Float</th>
            <th scope="col">Date</th>
            <th scope="col">Boolean</th>
            <th scope="col">Actions</th>
        </tr>
        <% data.forEach((data)=>{ %>
        <tr>
            <td><%= data.id %></td>
            <td><%= data.dataString %></td>
            <td><%= data.dataInteger %></td>
            <td><%= data.dataFloat %></td>
            <td><%= data.dataDate %></td>
            <td><%= data.dataBoolean %></td>
            <td><a class="btn btn-success" href="edit/<%= data.id %>">Edit</a>
                <a class="btn btn-danger" href="deleted/<%= data.id %>">Delete</a>
            </td>
        </tr>
        <% }); %>
    </table>

    <a class="btn btn-primary" href="/add">ADD</a>
    
</body>
</html> 